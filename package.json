{
  "name": "number-crunch",
  "version": "1.2.3",
  "description": "Fast arbitraty-precision integer arithmetic library. Used for large-number calculations including finding large prime numbers, performing modular exponentiation and other arithmetic operations.",
  "main": "crunch.js",
  "scripts": {
    "test": "mocha test",
    "coverage" : "istanbul cover ./node_modules/mocha/bin/_mocha --report lcov -- -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vukicevic/crunch.git"
  },
  "keywords": [
    "bignum",
    "arithmetic",
    "maths",
    "primes",
    "modulo",
    "exponentiation",
    "crypto",
    "integer"
  ],
  "author": "Nenad Vukicevic <vukicevic@gmail.com> (https://github.com/vukicevic)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vukicevic/crunch/issues"
  },
  "homepage": "https://github.com/vukicevic/crunch",
  "devDependencies": {
    "istanbul": "latest",
    "mocha": "latest",
    "mocha-lcov-reporter": "latest",
    "should": "latest"
  },
  "config": {
    "blanket": {
      "pattern": [ "" ],
      "data-cover-never": [ "node_modules", "test" ]
    }
  }
}
